{
  "name": "basket",
  "version": "1.0.0",
  "description": "A simple css framework",
  "author": "Omar Quazi <omar@quazi.co>",
  "license": "MIT",
  "homepage": "https://getbasket.co",
  "repository": "blacksector/basket",
  "main": "index.js",
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer --no-map.inline --autoprefixer.browsers \"last 1 versions\" -r dist/*.css",
    "banner": "banner-cli dist/basket.css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && run-s sass autoprefixer banner",
    "sass": "node-sass --output-style expanded src/basket.sass dist/basket.css && node-sass --output-style compressed src/basket.sass dist/basket.min.css",
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "copy": "rimraf docs/dist && copyfiles -u 1 dist/*.css docs/dist"
  },
  "keywords": [
    "css",
    "css3",
    "framework",
    "bootstrap",
    "front-end",
    "flexbox",
    "html",
    "html5",
    "responsive",
    "mobile",
    "websites",
    "sass",
    "scss"
  ],
  "devDependencies": {
    "autoprefixer": "^10.0.4",
    "banner-cli": "^0.14.3",
    "copyfiles": "^2.4.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "last 3 versions"
  ]
}
